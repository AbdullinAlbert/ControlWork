    <string name="search_criteria_for_edit_delete_data">SELECT Result._id, Employer._id, Employer.fio, Firm._id, Firm.description, TypeOfWork._id, TypeOfWork.description, PlaceOfWork._id, PlaceOfWork.description, strftime(\"%d.%m.%Y\", Result.Date, \"unixepoch\") AS Date, Result.Value, Result.Note, Result.idResultType, ResultType.ResultType
        FROM Result JOIN Employer
        ON Result.IDEmployer=Employer._id
        JOIN Firm ON Firm._id=Result.IDFirm
        JOIN TypeOfWork ON TypeOfWork._id=Result.IDToW
        JOIN PlaceOfWork ON PlaceOfWork._id=Result.IDPoW
        LEFT JOIN ResultType ON Result.idResultType=ResultType._id</string>
